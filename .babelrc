{
    "presets": [
      ["@babel/env", {
        "targets": {
          "node": "8.11.2"
        },
        "useBuiltIns": "usage"
      }]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "babel-plugin-autobind-class-methods"
    ]
  }